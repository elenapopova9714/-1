    "def evaluate (x):\n",
    "    U=1\n",
    "    S=0\n",
    "    for i in range(len(x)):\n",
    "        if x[i] % 2 == 0:  \n",
    "            U=U*x[i]\n",
    "        else:            \n",
    "            S=S+x[i]\n",
    "    print (S-U)  "
   ]
